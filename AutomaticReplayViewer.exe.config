    <add key="GGXrd HUD keyboard input" value="J" />
    <add key="GGXrd Inputs keyboard input" value="L" />
    <add key="GGXrd Hide Window" value="True" />
    <add key="GGXrd Hide HUD" value="False" />
    <add key="GGXrd Hide Inputs" value="False" />
    <!--GGXrd Pointers-->
    <add key="GGXrdMenuState" value="0x177FB8C" />
    <add key="GGXrdOutroPlaying" value="0x1937DEC" />
    <add key="GGXrdCursor" value="0x0173A670,0x14,0x4,0x60,0x1BC" />
