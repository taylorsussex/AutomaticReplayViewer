    <add key="ROAMenuState" value="0x3486548" />
    <add key="ROACursorX" value="0x03486520,0x80,0xA0" />
    <add key="ROACursorY" value="0x03486520,0x80,0xA4" />
    <!--Relevant Key Bindings in BBTag-->
    <add key="BBTag Up keyboard input" value="W" />
    <add key="BBTag Confirm keyboard input" value="J" />
    <add key="BBTag Gauge keyboard input" value="U" />
    <add key="BBTag Window keyboard input" value="I" />
    <add key="BBTag Hide Gauge" value="False" />
