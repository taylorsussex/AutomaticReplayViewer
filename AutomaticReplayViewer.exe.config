<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--Relevant Key Bindings in SG-->
    <!--It is essential that the keys here match the bindings that you have in SG-->
    <!--All key settings must be as they are for System.Windows.Forms.Keys-->
    <!--This list can be checked here msdn.microsoft.com/en-us/library/system.windows.forms.keys.aspx-->
    <add key="SG LP keyboard input" value="A" />
    <add key="SG LK keyboard input" value="Z" />
    <add key="SG MP keyboard input" value="S" />
    <add key="SG Right keyboard input" value="Right" />
    <add key="DefaultDisplayHitboxes" value="False" />
    <add key="DefaultDisplayInputs" value="False" />
    <add key="DefaultDisplayAttackData" value="False" />
    <!--SG Pointers-->
    <add key="SGMenuState" value="3934232" />
    <!--Relevant Key Bindings in ROA-->
    <add key="ROA Start keyboard input" value="Return" />
    <add key="ROA L keyboard input" value="A" />
